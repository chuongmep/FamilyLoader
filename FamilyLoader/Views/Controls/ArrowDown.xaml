<UserControl x:Class="Gensler.Revit.FamilyLoader.Views.Controls.ArrowDown"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Gensler.Revit.FamilyLoader.Views.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="24" d:DesignWidth="24">
    <Viewbox Stretch="Uniform">
        <Canvas Width="24" Height="24">
            <Canvas.RenderTransform>
                <TranslateTransform X="0" Y="0"/>
            </Canvas.RenderTransform>
            <Canvas.Resources/>
            <Path Fill="#FFA9A9A9">
                <Path.Data>
                    <PathGeometry Figures="M2 8 l10 10 10-10 z" FillRule="NonZero"/>
                </Path.Data>
            </Path>
        </Canvas>
    </Viewbox>
</UserControl>
